TARGET			=	MagicSave
PRX_EXPORTS		=	exports.exp


USE_KERNEL_LIBC		=	1
USE_KERNEL_LIBS		=	1

PSP_NID_RESOLVE		= 	

DEBUG			=	0


OBJS			=	exports.o		\
				MS_PLUGIN_LIB.o 	\
				./include/psf.o		\
				./include/hook.o	\
				./include/syslibc.o	\
				common.o		\
				main.o			\
				func.o			\
				menu.o			\
				plugin.o		\
				cfg.o			\
				log.o



ifneq ($(PSP_NID_RESOLVE),)
OBJS			+=	./NID_Resolve/$(PSP_NID_RESOLVE).o
endif



INCDIR			=	./include
CFLAGS			=	-Os -G 0 -Wall -fno-strict-aliasing -DLNGJPN -D_DEBUG=$(DEBUG)
ASFLAGS			=	$(CFLAGS)


LIBDIR			= 	
LDFLAGS			=	-mno-crt0 -nostartfiles
LIBS			=	-lini -lmenuj



PSPSDK=$(shell psp-config --pspsdk-path)
include $(PSPSDK)/lib/build_prx.mak

